{
    "name"              : "rudra/event-dispatcher",
    "description"       : "Rudra framework",
    "type"              : "library",
    "keywords"          : [
        "rudra",
        "event-dispatcher"
    ],
    "license"           : "GPL-3.0-or-later",
    "authors"           : [
        {
            "name"     : "Danila Korotkov (Jagepard)",
            "email"    : "jagepard@yandex.ru",
            "homepage" : "https://jagepard.ru",
            "role"     : "Developer"
        }
    ],
    "support"           : {
        "email" : "jagepard@yandex.ru",
        "skype" : "jagepard.ru"
    },
    "require"           : {
        "php"               : ">=7.2",
        "rudra/container"   : "self.version"
    },
    "require-dev"       : {
        "phpunit/phpunit": "^7",
    },
    "autoload"          : {
        "psr-4" : {
            "Rudra\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4": {
            "Rudra\\Tests\\" : "tests/"
        }
    },
    "minimum-stability" : "dev",
    "config"            : {
        "optimize-autoloader" : true,
        "prefer-stable"       : true
    }
}
